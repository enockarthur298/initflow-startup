{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/remix@latest"
    },
    {
      "src": "api/vercel-serverless/index.py",
      "use": "@vercel/python@latest",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.9"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/vercel-serverless/index.py"
    },
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ]
}
